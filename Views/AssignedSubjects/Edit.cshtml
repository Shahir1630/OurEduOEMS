@model AssignedSubjects

@{
    ViewBag.Title = "Edit Assign Subject";
}

<h4>Edit Assign Subject</h4>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <input asp-for="Id" type="hidden" class="form-control" />

            <div class="form-group">
                <label asp-for="AssignedClassesId" class="control-label"></label>
                <select asp-for="AssignedClassesId" class="form-control" asp-items="ViewBag.AssignClassId">
                    <option selected>Select One </option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="SubjectsId" class="control-label"></label>
                <select asp-for="SubjectsId" class="form-control" asp-items="ViewBag.SubjectId">
                    <option selected>Select One </option>
                </select>
            </div>

            <div class="form-group">
                <label asp-for="Year" class="control-label"></label>
                <input asp-for="Year" class="form-control" />
                <span asp-validation-for="Year" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

